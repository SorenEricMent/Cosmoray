{
    "manifest_version": 2,
    "name": "Coxmoray",
    "description": "百度贴吧优化插件 By WinslowEric",
    "version": "0.0.1",
    "permissions": [
        "tabs",
        "https://api.cr.wlinc.cc",
        "https://data.cr.wlinc.cc",
        "https://verify.cr.wlinc.cc",
        "https://rt.cr.wlinc.cc",
        "https://tieba.baidu.com/",
        "contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage"
    ],
    "icons":
	{
		"16": "images/icon.png",
		"48": "images/icon.png",
		"128": "images/icon.png"
	},
    "content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["scripts/jquery.js","scripts/kernal.js"],
			"run_at": "document_start"
		}
	],
    "browser_action": 
	{
		"default_icon": "images/icon.png",
		"default_title": "百度贴吧优化插件 By WinslowEric",
		"default_popup": "index.html"
	},
	"homepage_url": "https://www.winsloweric.cn"
}